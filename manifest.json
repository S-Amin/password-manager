{
  "manifest_version": 3,
  "name": "Password Manager",
  "description": "Base Level Extension",
  "version": "1.0",
  "action": {
    "default_popup": "index.html",
    "default_title": "pass"
  },
  "content_scripts": [
    {
      "js": ["dist/extension/bundle.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "permissions": ["activeTab", "tabs", "storage"],
  "background": {
    "service_worker": "dist/extension/service-worker.js",
    "type": "module"
  }
}
