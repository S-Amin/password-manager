{
  "name": "password-manager",
  "packageManager": "yarn@4.0.2",
  "private": true,
  "scripts": {
    "ext": "yarn node --watch index.js",
    "server": "tsc --project tsconfig.server.json && node ./dist/server/server.mjs",
    "build": "yarn build:frontend && webpack",
    "build:watch": "yarn build:frontend && webpack --watch & tsc --watch",
    "build:frontend": "yarn clean && tsc --project tsconfig.frontend.json",
    "clean": "rm -rf dist && rm -rf public/js"
  },
  "browser": {
    "./myFile.js": "./dist/myFile.js"
  },
  "dependencies": {
    "@fastify/static": "^6.12.0",
    "fastify": "^4.24.3",
    "js-sha256": "^0.10.1"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.253",
    "@types/node": "^20.10.4",
    "pnp-webpack-plugin": "^1.7.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  }
}
